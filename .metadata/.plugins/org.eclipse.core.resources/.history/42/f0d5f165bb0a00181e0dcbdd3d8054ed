#include <msp430.h> 

#define RED_ON	0x0001
#define RED_OFF	0x0000
#define DEVELOPMENT	0x5A80
#define ENABLE_IO	0xFFFE

/*
 * main.c
 */
int main(void) {
    WDTCTL = WDTPW | WDTHOLD;	// Stop watchdog timer

    int x, y;

    y = 0;

    for(x=0; x<10; x=x+1)
    {
    	y = y + x;
    }

    while(1);
}
